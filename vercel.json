{
  "version": 2,
  "builds": [
    {
      "src": "feedback-mini-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "home-page-mini-app/package.json",
      "use": "@vercel/static-build", 
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/feedback-mini-app/(.*)",
      "dest": "/feedback-mini-app/$1"
    },
    {
      "src": "/home-page-mini-app/(.*)",
      "dest": "/home-page-mini-app/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
